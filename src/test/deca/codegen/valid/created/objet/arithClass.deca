// arithmetic 
class Plus{
    int plus(int a, int b){
        return a +b;
    }
}

class Minus{
    int minus(int a, int b){
        return a - b;
    }
}


class Mult{
    int mult(int a, int b){
        return a *b;
    }
}

class Divide{
    int divide(int a, int b){
        return a /b;
    }
}

{
    Plus p = new Plus();
    int result = p.plus(10,10);
    Mult mu = new Mult();
    int result2 = mu.mult(10,10);
    Minus mi = new Minus();
    int result3 = mi.minus(10,10);
    Divide di = new Divide();
    int result4 = di.divide(10,10);

    if(result == 20){
        println("passed 1");
    } else {
        println("error 1");
    }

    
    if(result2 == 100){
        println("passed 2");
    } else {
        println("error 2");
    }

    
    if(result3 == 0){
        println("passed 3");
    } else {
        println("error 3");
    }

    
    if(result4 == 1){
        println("passed 4");
    } else {
        println("error 4");
    }
}